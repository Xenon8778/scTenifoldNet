Package: scTenifoldNet
Type: Package
Title: Construct and Compare Gene Regulatory Networks (GRN) from Single-Cell Transcriptomic Data
Version: 0.0.1
Authors@R: c(person(given = "Daniel", family = "Osorio", email = "dcosorioh@tamu.edu", role = c("aut","cre")),
             person(given = "Yan", family = "Zhong", role = c("aut","ctb")),
             person(given = "Guanxun", family = "Li", role = c("aut","ctb")),
